<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="prefereciacorusuario.cursoandroid.com.petcare.RacaoPeixeActivity"
    android:background="#3bd16d">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Rações "
        android:id="@+id/textView11"
        android:textSize="30dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <ImageView
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:id="@+id/imageView2"
        android:background="@drawable/peixinho"
        android:layout_below="@+id/textView12"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Ração Tropical para Peixe Cichlid Color Flakes 20g"
        android:id="@+id/textView12"
        android:textSize="15dp"
        android:textIsSelectable="false"
        android:layout_below="@+id/textView11"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="42dp"
        android:textColor="#050000" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=" Descrição Cichlid Color é um alimento em flocos que aumenta a cor com alto teor de proteína para alimentação diária de ciclídeos onívoros e carnívoros, incluindo muitas espécies de cíclidos de Malawi. A adição de urtiga facilita o metabolismo eo bom funcionamento do pâncreas e do fígado. O sistema de imunidade é reforçado por um forte estimulante imune natural – beta-glucano – e vitamina C estabilizada. A coloração intensa e brilhante é o efeito de carotenóides naturais de krill e algas, incluindo a spirulina. A alimentação regular com Cichlid Color, caracterizada por uma grande variedade de nutrientes, garante a excelente saúde e condição do peixe, bem como uma esplêndida coloração.   Ingredientes: peixe e derivados de peixe, cereais, extratos de proteínas vegetais, derivados de origem vegetal (farinha de urtiga 2,5%), moluscos e crustáceos (farinha de krill 1,5% mínimo, farinha de calamar 1,5%), algas (Spirulina platensis min 1%), leveduras (incluindo Beta-1,3 / 1,6-Glucan 1 000 mg / kg), óleos e gorduras, minerais (incluindo zeólito 1%)   Modo de uso: oferecer de 2 a 3 vezes por dia em quantidades que possam ser consumidas em poucos minutos."
        android:id="@+id/textView13"
        android:layout_alignTop="@+id/imageView2"
        android:layout_toRightOf="@+id/imageView2"
        android:layout_alignBottom="@+id/imageView2"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/botaoAdicionarRacaoPeixe1"
        android:id="@+id/botaoAdicionarOCarrinhoRacaoPeixe"
        android:layout_below="@+id/imageView2"
        android:layout_toStartOf="@+id/textView11"
        android:layout_toLeftOf="@+id/textView11" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Ração para Peixe Alcon Spirulina Flakes 10g"
        android:id="@+id/textView14"
        android:layout_below="@+id/botaoAdicionarOCarrinhoRacaoPeixe"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textSize="15dp"
        android:textColor="#070000" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/descri_o_ra_o_spirulina_um_alimento_rico_em_nutrientes_tem_presen_a_marcante_de_spirulina_especial_para_peixes_ornamentais_tropicais_ou_marinho_a_ra_o_spirulina_t_o_completa_que_possui_prebi_ticos_que_ajudam_no_aumento_da_flora_intestinal_ben_fica_fundamental_para_manter_a_sa_de_do_seu_peixinho_composi_o_b_sica_farelo_de_soja_farinha_de_peixe_farinha_de_lula_espirulina_desidratada_10_1_f_cula_de_mandioca_farinha_de_lula_adsorvente_de_micotoxinas_levedura_leo_de_soja_refinado_premix_vitam_nico_mineral_cloreto_de_s_dio_leo_de_peixe_farinha_de_minhoca_premix_mineral_0_1_aditivo_prebi_tico_0_09_antioxidantes_etoxiquin_propilgalato_cido_c_trico_bha_bht_aditivo_enzim_tico_0_05_esp_cies_doadoras_de_gene_agrobacterium_tumefaciens_bacillus_thuringiensis_streptomyces_viridochromogenes_n_veis_de_garantia_umidade_m_x_100g_kg_10_prote_na_bruta_min_420_g_kg_42_extrato_de_et_reo_min_50g_kg_5_mat_ria_fibrosa_m_x_40g_kg_4_mat_ria_mineral_m_x_150_g_kg_15_c_lcio_m_x_30_g_kg_3_c_lcio_min_16g_kg_1_6_fosforo_min_8000_mg_kg_0_8_mananoligossacar_deo_min_200mg_kg_0_02_protease_min_350_u_kg_amilase_min_15_u_kg_celulase_min_20_u_kg_modo_de_uso_fornecer_quantidade_a_ser_totalmente_consumida_em_at_cinco_minutos_de_duas_a_quatro_vezes_por_dia"
        android:id="@+id/textView15"
        android:layout_below="@+id/textView14"
        android:layout_toEndOf="@+id/botaoAdicionarCarrinhoPeixe2"
        android:layout_above="@+id/botaoAdicionarCarrinhoPeixe2"
        android:layout_alignLeft="@+id/textView13"
        android:layout_alignStart="@+id/textView13" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/adicionarRacaoPeixe2"
        android:id="@+id/botaoAdicionarCarrinhoPeixe2"
        android:layout_below="@+id/imageView3"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_toStartOf="@+id/textView13"
        android:layout_alignRight="@+id/botaoAdicionarOCarrinhoRacaoPeixe"
        android:layout_alignEnd="@+id/botaoAdicionarOCarrinhoRacaoPeixe" />

    <ImageView
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:id="@+id/imageView3"
        android:layout_below="@+id/textView14"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:background="@drawable/download" />

</RelativeLayout>
